    }, {
      "name" : "sampleId",
      "type" : [ "null", {
        "type" : "array",
        "items" : "string"
      } ],
      "doc" : "Sample Id (e.g, LP00012645_5GH))"
    }, {
      "name" : "assignedICD10",
      "type" : [ "null", "string" ],
      "doc" : "assigned ICD10 code"
    } ]
  }, {
