      "name" : "tumorSubType",
      "type" : [ "null", "string" ],
      "doc" : "Tumor Subtype"
    }, {
      "name" : "preservationMethod",
      "type" : [ "null", "PreservationMethod" ],
      "doc" : "Preservation method"
    }, {
      "name" : "phase",
      "type" : [ "null", "Phase" ],
      "doc" : "Tumor Phase"
    }, {
      "name" : "method",
      "type" : [ "null", "Method" ],
      "doc" : "Method"
    }, {
      "name" : "tumorPurity",
      "type" : [ "null", "double" ],
      "doc" : "Sample tumorPurity"
    }, {
      "name" : "tumorContent",
      "type" : [ "null", "string" ],
      "doc" : "Sample TumorContent will be a string"
    }, {
      "name" : "grade",
      "type" : [ "null", "string" ],
